{
  "name": "milochau.contacts.client",
  "version": "0.1.0",
  "description": "Milochau Contacts Client",
  "author": "Antoine Milochau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amilochau/aws-contacts"
  },
  "homepage": "https://github.com/amilochau/aws-contacts#readme",
  "bugs": {
    "url": "https://github.com/amilochau/aws-contacts/issues"
  },
  "funding": "https://github.com/sponsors/amilochau",
  "company": "Milochau",
  "copyright": "Copyright Â© Milochau",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite serve --port 3000",
    "preview": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix"
  },
  "dependencies": {
    "@amilochau/core-vue3": "^0.5.6"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/vue3-recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "rules": {
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  }
}
